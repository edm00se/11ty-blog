{
	"name": "11ty-blog",
	"version": "8.0.0",
	"description": "A starter repository for a blog web site using the Eleventy site generator.",
	"scripts": {
		"build": "env NODE_ENV=production npx @11ty/eleventy",
		"build:ghpages": "env NODE_ENV=production npx @11ty/eleventy --pathprefix=/11ty-blog/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/edm00se/11ty-blog.git"
	},
	"author": {
		"name": "Eric McCormick",
		"url": "https://edm00se.codes/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=16"
	},
	"devDependencies": {
		"@11ty/eleventy": "3.0.0-alpha.4",
		"@11ty/eleventy-img": "^3.1.1",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-bundle": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"eleventy-plugin-embed-everything": "^1.17.0",
		"eleventy-plugin-toc": "^1.1.5",
		"luxon": "^3.3.0",
		"markdown-it-anchor": "^8.6.7",
		"markdown-it-attrs": "^4.1.6"
	}
}
